<?xml version="1.0" encoding="UTF-8" ?>
 
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	
	<package name="jigou_default" extends="struts-default" >

		<action name="jigouall" class="securityccb.jigou.action.JigouAction">
			<result name="success">/page/anbaobutiaozhuan.jsp</result>
			<result name="success?">/page/testupfile.jsp</result>
		</action>
		<action name="anbaobutojigou" class="securityccb.jigou.action.AnbaobuToJigou">
			<result name="benji">/page/beforeanbaobu.jsp</result>
			<result name="success??">/page/anbaobutiaozhuan.jsp</result>
			<result name="success">/page/anbaobu/gejigou.jsp</result>
		</action>	
		<action name="jigoutoanbaobu" class="securityccb.jigou.action.JigouToAnbaobu">
			<result name="success">/page/anbaobutiaozhuan.jsp</result>	
		</action>
		<action name="jigoulist" class="securityccb.jigou.action.JigouList">
			<result name="success">/page/anbaobu/jigoulist.jsp</result>	
		</action>
		<action name="jigoutomod" class="securityccb.jigou.action.JiGouToMod">
			<result name="success">/page/anbaobu/jigoumod.jsp</result>	
		</action>
		<action name="jigouaddbefore" class="securityccb.jigou.action.JiGouAddBefore">
			<result name="success">/page/anbaobu/jigouadd.jsp</result>	
		</action>
		<action name="jigouadd" class="securityccb.jigou.action.JiGouAdd">
			<result name="success">/page/anbaobu/jigoubefore.jsp</result>	
		</action>
	</package>
</struts>    
