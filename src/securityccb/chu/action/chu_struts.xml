<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	
	<package name="chu_default" extends="struts-default" >

		<action name="chu" class="securityccb.chu.action.ChuAction">
			<result name="success">/page/chu.jsp</result>
			<result name="find_all_success">/page/system/chu.jsp</result>
			
		</action>
		<action name="chuDel" class="securityccb.chu.action.chuDel">
			<result name="success">/page/system/chu_del_success.jsp</result>
			<result name="unsuccess">/page/system/chu_del_success.jsp</result>			
		</action>
		<action name="chuUpdate" class="securityccb.chu.action.chuUpdate">
			<result name="success">/page/system/chu_del_success.jsp</result>			
		</action>
		<action name="chuAdd" class="securityccb.chu.action.chuAdd">
			<result name="success">/page/system/chu_del_success.jsp</result>			
		</action>

	</package>
</struts>    
